{
  "ecmaFeatures": {
        "jsx": true
    },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": ["error", { "variables": false }]
  },
  "plugins": [
    "react"
  ]
}
